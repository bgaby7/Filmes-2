UPDATE `ENTRETERIMENTO`
SET `RANKING_NOME` = 
    CASE 
        WHEN `NOME` = 'Stranger Things' THEN 'Netflix'
        WHEN `NOME` = 'Black Mirror' THEN 'Netflix'
        WHEN `NOME` = 'The Irishman' THEN 'Netflix'
        WHEN `NOME` = 'Breaking Bad' THEN 'Netflix'
        WHEN `NOME` = 'The Crown' THEN 'Netflix'
        WHEN `NOME` = 'Inception' THEN 'Netflix'
        WHEN `NOME` = 'Friends' THEN 'Netflix'
        WHEN `NOME` = 'The Godfather' THEN 'Netflix'
        WHEN `NOME` = 'La Casa de Papel' THEN 'Netflix'
        WHEN `NOME` = 'Pulp Fiction' THEN 'Netflix'
        WHEN `NOME` = 'The Witcher' THEN 'Netflix'
        WHEN `NOME` = 'The Matrix' THEN 'Netflix'
        WHEN `NOME` = 'Money Heist' THEN 'Netflix'
        WHEN `NOME` = 'Forrest Gump' THEN 'Netflix'
        WHEN `NOME` = 'Vikings' THEN 'Netflix'
        WHEN `NOME` = 'The Dark Knight' THEN 'Netflix'
        WHEN `NOME` = 'Grey’s Anatomy' THEN 'Star'
        WHEN `NOME` = 'The Shawshank Redemption' THEN 'HBO'
        WHEN `NOME` = 'Gladiador' THEN 'Amazon Prime'
        WHEN `NOME` = 'O Senhor dos Anéis: A Sociedade do Anel' THEN 'Disney'
        WHEN `NOME` = 'Clube da Luta' THEN 'Disney'
        WHEN `NOME` = 'Cidade de Deus' THEN 'HBO'
        WHEN `NOME` = 'Kill Bill: Volume 1' THEN 'HBO'
        WHEN `NOME` = 'Entrevista com o Vampiro' THEN 'Disney'
        WHEN `NOME` = 'Bastardos Inglórios' THEN 'Amazon Prime'
        WHEN `NOME` = 'Sr. e Sra. Smith' THEN 'Amazon Prime'
        WHEN `NOME` = 'Tropa de Elite' THEN 'Amazon Prime'
        WHEN `NOME` = 'Central do Brasil' THEN 'Disney'
        WHEN `NOME` = 'O Auto da Compadecida' THEN 'Disney'
        ELSE 'Plataforma Desconhecida'
    END;
